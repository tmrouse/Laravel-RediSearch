{
    "name": "ethanhann/laravel-redisearch",
    "type": "library",
    "description": "RediSearch integration for Laravel",
    "keywords": [
        "redis", "redisearch", "laravel", "scout", "search", "fulltext", "geosearch", "php"
    ],
    "homepage": "https://github.com/ethanhann/Laravel-RediSearch",
    "license": "MIT",
    "authors": [
        {
            "name": "Ethan Hann",
            "email": "ethanhann@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ehann\\LaravelRediSearch\\": "src"
        }
    },
    "require": {
        "laravel/scout": "^3.0|^4.0|^5.0|^6.0|^7.0|^8.0",
        "ethanhann/redisearch-php": "^1.0.0",
        "ethanhann/redis-raw": "^0.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "mockery/mockery": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ehann\\LaravelRediSearch\\RediSearchServiceProvider"
            ]
        }
    }
}
